    <servlet-class>com.sahakari.controller.UploadController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UploaderController</servlet-name>
    <url-pattern>*.upload</url-pattern>
  </servlet-mapping>
